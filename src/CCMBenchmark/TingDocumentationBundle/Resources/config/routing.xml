<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="ccm_benchmark_ting_documentation_homepage" path="/hello/{name}">
        <default key="_controller">CCMBenchmarkTingDocumentationBundle:Default:index</default>
    </route>

    <route id="ccm_benchmark_ting_documentation_homepage" path="/doc/{name}">
        <default key="_controller">CCMBenchmarkTingDocumentationBundle:Doc:index</default>
        <requirement key="name">.+</requirement>
    </route>
</routes>
